{
  "name": "jsfive",
  "version": "0.4.0",
  "description": "A pure javascript HDF5 file reader, based on pyfive",
  "main": "./dist/cjs/index.js",
  "module": "./esm/high-level.js",
  "exports": {
    ".": {
      "browser": "./esm/high-level.js",
      "import": "./esm/high-level.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "browser": "./esm/high-level.js",
  "scripts": {
    "build": "npm run build_browser && npm run build_esm && npm run build_commonjs && npm run build_node",
    "build_browser": "esbuild index.js --bundle --sourcemap --target=es2020 --outfile=dist/browser/hdf5.js --format=iife --global-name=hdf5 --platform=browser --external:./esm/node-file-source.js",
    "build_commonjs": "esbuild index.js --bundle --sourcemap --target=es2020 --outfile=dist/cjs/index.js --format=cjs --platform=node",
    "build_esm": "esbuild index.js --bundle --format=esm --outfile=dist/esm/index.mjs --platform=neutral --external:fs/promises",
    "build_node": "esbuild esm/node-file-source.js --bundle --format=esm --outfile=dist/esm/node-file-source.mjs --platform=node"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/usnistgov/jsfive"
  },
  "keywords": [
    "hdf5",
    "javascript",
    "es6",
    "browser"
  ],
  "author": "Brian B. Maranville",
  "license": "SEE LICENSE IN LICENSE.txt",
  "bugs": {
    "url": "https://github.com/usnistgov/jsfive/issues"
  },
  "homepage": "https://github.com/usnistgov/jsfive#readme",
  "dependencies": {
    "pako": "^2.0.4"
  },
  "devDependencies": {
    "esbuild": "^0.25.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
