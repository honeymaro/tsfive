{
  "name": "tsfive",
  "version": "1.0.0",
  "description": "A pure TypeScript HDF5 file reader, based on pyfive",
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.mjs",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.js"
    },
    "./node": {
      "types": "./dist/types/node-file-source.d.ts",
      "import": "./dist/esm/node-file-source.mjs"
    }
  },
  "browser": "./dist/browser/hdf5.js",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "build:types": "tsc --declaration --emitDeclarationOnly --outDir dist/types",
    "build:esm": "esbuild src/index.ts --bundle --format=esm --outfile=dist/esm/index.mjs --platform=neutral --external:fs/promises --sourcemap",
    "build:cjs": "esbuild src/index.ts --bundle --format=cjs --outfile=dist/cjs/index.js --platform=node --sourcemap",
    "build:browser": "esbuild src/index.ts --bundle --format=iife --global-name=hdf5 --outfile=dist/browser/hdf5.js --platform=browser --external:./src/node-file-source.ts --sourcemap",
    "build:node": "esbuild src/node-file-source.ts --bundle --format=esm --outfile=dist/esm/node-file-source.mjs --platform=node",
    "build": "pnpm run clean && pnpm run lint && pnpm run typecheck && pnpm run build:types && pnpm run build:esm && pnpm run build:cjs && pnpm run build:browser && pnpm run build:node",
    "clean": "rimraf dist",
    "test": "node --test test/tests.mjs",
    "prepublishOnly": "pnpm run build && pnpm run test",
    "prepare": "husky"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/honeymaro/tsfive"
  },
  "keywords": [
    "hdf5",
    "typescript",
    "javascript",
    "es6",
    "browser"
  ],
  "author": "Hanseulmaro Kim <honeymaro@live.co.kr>",
  "contributors": [
    "Brian B. Maranville (original jsfive author)"
  ],
  "license": "SEE LICENSE IN LICENSE.txt",
  "bugs": {
    "url": "https://github.com/honeymaro/tsfive/issues"
  },
  "homepage": "https://github.com/honeymaro/tsfive#readme",
  "dependencies": {
    "pako": "^2.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^25.0.3",
    "@types/pako": "^2.0.4",
    "esbuild": "^0.27.2",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "rimraf": "^6.1.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.51.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}